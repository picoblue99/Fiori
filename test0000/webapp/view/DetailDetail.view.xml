<mvc:View controllerName="test0000.controller.DetailDetail" 
		xmlns:l="sap.ui.layout"
		xmlns:core="sap.ui.core"
        xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" 
	xmlns:f="sap.ui.layout.form">
    <Page title="Detail Detail Page" showNavButton="true" navButtonPress="onNavButtonPress">
        <VBox class="sapUiSmallMargin">
            <f:SimpleForm
                editable="false"
                layout="ColumnLayout"
                title="Supplier"
                columnsM="2"
                columnsL="3"
                columnsXL="4"
                >
                <f:content>
                    <core:Title text="Contact" />
                    <Label text="Twitter" />
                    <Text text="{Twitter}" />
                    <Label text="Email" />
                    <Text text="{Email}" />
                    <Label text="Tel." />
                    <Text text="{Tel}" />
                    <Label text="SMS" />
                    <Text text="{Sms}" />
                    <Label text="Mobile" />
                    <Text text="{Mobile}" />
                    <Label text="Pager" />
                    <Text text="{Pager}" />
                    <Label text="Fax" />
                    <Text text="{Fax}" />
                </f:content>
            </f:SimpleForm>
        </VBox>
    </Page>
</mvc:View>
